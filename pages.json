{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"app-plus": {
					// 关闭反弹效果
					"bounce":"none",
					// 禁止原生导航 navigationStyle为custom，或titleNView为false
					"titleNView": false
				}
			}
		},
		{
			"path" : "pages/learn/learn",
			"style" : {}
		},
		{
			"path" : "pages/capacity/capacity",
			"style" : {}
		},
		{
			"path" : "pages/mine/mine",
			"style" : {
				"navigationBarTitleText":"我的",
				"navigationBarTextStyle": "white",
				"app-plus": {
					"bounce":"none",
					"titleNView":  {
						"autoBackButton": false,
						"backgroundColor": "#FF3E5C",
						"buttons": [
							// 右面
							{
								"color": "#FFFFFF",
								"colorPressed": "#BBBBBB",
								"float":"right",
								"fontSize":"22px",
								"fontSrc":"/static/font/icon.ttf",
								"text":"\ue600"
							}
						]
					}
				}
			}
		},
		{
			"path" : "pages/set-up/set-up",
			"style" : {
				"navigationBarTitleText":"设置",
				"app-plus": {
					"bounce": "none",
					"titleNView": {
						"titleSize": "14px",
					  "buttons": [
						 {
							 "type":"none"
						 }
					  ]
					}
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "安博爱学",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#FFFFFF"
	},
	"tabBar": {
		"color":"#757575",
		"selectedColor":"#F34064",
		"backgroundColor":"#FFFFFF",
		"borderStyle":"black",
		"list": [
			{
				"pagePath":"pages/index/index",
				"text":"选课",
				"iconPath":"static/tabbar/index.png",
				"selectedIconPath":"static/tabbar/index-active.png"
			},
			{
				"pagePath":"pages/learn/learn",
				"text":"学习",
				"iconPath":"static/tabbar/learn.png",
				"selectedIconPath":"static/tabbar/learn-active.png"
			},
			{
				"pagePath":"pages/capacity/capacity",
				"text":"功能",
				"iconPath":"static/tabbar/capacity.png",
				"selectedIconPath":"static/tabbar/capacity-active.png"
			},
			{
				"pagePath":"pages/mine/mine",
				"text":"我的",
				"iconPath":"static/tabbar/mine.png",
				"selectedIconPath":"static/tabbar/mine-active.png"
			}
		]
	}
}
